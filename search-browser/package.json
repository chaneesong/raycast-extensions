{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "search-browser",
  "title": "Search Browser",
  "description": "Search Browser extension",
  "icon": "command-icon.png",
  "author": "sch102403",
  "categories": [
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "saerch-browser",
      "title": "Saerch Browser",
      "subtitle": "Search Browser",
      "description": "Search Browser",
      "mode": "no-view",
      "arguments": [
        {
          "name": "url",
          "placeholder": "URL",
          "type": "text",
          "required": true
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.57.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
